        <source_script><![CDATA[answer = (function transformEntry(source) {

	var gc = new GlideRecord("sn_fin_legal_entity");
	gc.addQuery('erp_company_code', source.company_code);
	gc.query();
	if(gc.next()){
		var gl = new GlideRecord("sn_fin_gl_account");
		gl.addQuery('legal_entity', gc.getUniqueValue());
		gl.addQuery('gl_account', source.reconciliation_account_in_general_ledger);
		gl.query();
		if(gl.next()){
			return gl.getUniqueValue();	
		}
	}
	
})(source);]]></source_script>
